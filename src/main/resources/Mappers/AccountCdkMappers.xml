<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.boollan.dao.IAccountCdkDao">

    <!--配置实体类数据-->
    <resultMap id="BaseUserResult" type="com.boollan.domain.account_cdk">
        <id column="ID" property="id" jdbcType="VARCHAR"/>
        <result column="CDK" property="cdk" jdbcType="VARCHAR"/>
        <result column="EFFECTIVE" property="effective" jdbcType="VARCHAR"/>
        <result column="MONEY" property="money" jdbcType="VARCHAR"/>
        <result column="OVERDUETIME" property="overduetime" jdbcType="VARCHAR"/>
    </resultMap>
    <insert id="insertCdkInfo" parameterType="com.boollan.domain.account_cdk">
        insert into accout_cdk(cdk,money,effective,overduetime)values(#{cdk},#{money},#{effective},#{overduetime})
    </insert>
    <update id="updateCdkInfo" parameterType="com.boollan.domain.account_cdk">
        update accout_cdk set money=#{money},effective=#{effective},overduetime=#{overduetime} where cdk=#{cdk}
    </update>
    <select id="findAllCdkInfo" resultType="com.boollan.domain.account_cdk">
        select * from accout_cdk
    </select>
    <select id="finCdkInfoByCdk" resultType="com.boollan.domain.account_cdk">
        select * from accout_cdk where cdk=#{cdk}
    </select>

</mapper>